{
  "name": "kanbased",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "dev:backend": "turbo dev --filter=backend",
    "dev:frontend": "turbo dev --filter=frontend"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.9"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
